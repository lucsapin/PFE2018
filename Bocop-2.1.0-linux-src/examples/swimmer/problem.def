# This file defines all dimensions and parameters
# values for your problem :

# Initial and final time :
time.free string none
time.initial double 0
time.final double 25

# Dimensions :
state.dimension integer 6
control.dimension integer 2
algebraic.dimension integer 0
parameter.dimension integer 0
constant.dimension integer 0
boundarycond.dimension integer 12
constraint.dimension integer 0

# Discretization :
discretization.steps integer 500
discretization.method string midpoint

# Optimization :
optimization.type string single
batch.type integer 0
batch.index integer 0
batch.nrange integer 1
batch.lowerbound double 0
batch.upperbound double 0
batch.directory string none

# Initialization :
initialization.type string from_init_file
initialization.file string none

# Parameter identification :
paramid.type string false
paramid.separator string ,
paramid.file string no_directory
paramid.dimension integer 0

# Names :
state.0 string x
state.1 string y
state.2 string theta
state.3 string beta1
state.4 string beta3
state.5 string energy
control.0 string beta1dot
control.1 string beta3dot
boundarycond.0 string x(0)
boundarycond.1 string y(0)
boundarycond.2 string theta(0)
boundarycond.3 string E(0)
boundarycond.4 string x(T)
boundarycond.5 string y(T)
boundarycond.6 string theta(T)
boundarycond.7 string E(T)
boundarycond.8 string theta(T)-theta(0)
boundarycond.9 string beta1(T)-beta1(0)
boundarycond.10 string beta3(T)-beta3(0)
boundarycond.11 string beta1(0)

# Solution file :
solution.file string problem.sol

# Iteration output frequency :
iteration.output.frequency integer 0

